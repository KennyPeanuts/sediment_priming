points(cn$percC[mean_tough$Position == "Sed"], mean_tough$mean.tough[mean_tough$Position == "Sed"], pch = 19)
abline(tough_by_cn_mod)
tough_by_cn_pos_mod <- lm(mean.tough ~ cn$percC * mean_tough$Position)
