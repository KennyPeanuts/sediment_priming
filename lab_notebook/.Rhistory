leaf.initial <- read.table("./data/inital_om.csv", header = T, sep = ",")
leaf.final <- read.table("./data/final_AFDM.csv", header = T, sep = ",")
init.leaf.mass <- leaf.initial$om.mass[leaf.initial$sample == "leaf"]
